{
  "name": "@eik/cli",
  "version": "2.0.0-next.6",
  "description": "Cli tool for publishing assets",
  "main": "./classes/index.js",
  "bin": {
    "eik": "index.js"
  },
  "files": [
    "CHANGELOG.md",
    "package.json",
    "readme.md",
    "index.js",
    "commands",
    "classes",
    "utils",
    "formatters"
  ],
  "scripts": {
    "test": "HTTP_PORT=0 LOG_LEVEL=fatal tap --no-coverage-report --no-cov --timeout 0 --test-regex=.test.js --test-ignore=node_modules",
    "test:integration": "HTTP_PORT=0 LOG_LEVEL=fatal tap --no-coverage-report --no-cov --timeout 0 test/integration/**/*.test.js",
    "test:unit": "HTTP_PORT=0 LOG_LEVEL=fatal tap --no-coverage-report --no-cov --timeout 0 test/*.test.js",
    "test:tasks": "HTTP_PORT=0 LOG_LEVEL=fatal tap --no-coverage-report --no-cov --timeout 0 test/tasks/*.test.js",
    "test:ci": "npm run test:unit  -- --jobs=1 && npm run test:tasks -- --jobs=1 && npm run test:integration -- --jobs=1",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier -c .",
    "format:fix": "prettier -w .",
    "fix": "npm run lint:fix && npm run format:fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@eik/common": "^4.0.0-next.3",
    "abslog": "2.4.0",
    "boxen": "5.0.1",
    "bytes": "3.1.0",
    "chalk": "4.1.2",
    "copy": "0.3.2",
    "date-fns": "2.23.0",
    "form-data": "4.0.0",
    "glob": "7.1.7",
    "gzip-size": "6.0.0",
    "make-dir": "3.1.0",
    "node-fetch": "2.6.1",
    "ora": "5.4.1",
    "pkg-dir": "5.0.0",
    "read-pkg-up": "7.0.1",
    "rimraf": "3.0.2",
    "semver": "7.3.5",
    "ssri": "8.0.1",
    "tar": "6.1.7",
    "temp-dir": "2.0.0",
    "yargs": "16.2.0",
    "yargs-parser": "20.2.9"
  },
  "devDependencies": {
    "@eik/core": "1.2.18",
    "@eik/service": "1.2.78",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-prettier": "3.4.0",
    "fastify": "3.20.1",
    "fastify-cors": "5.2.0",
    "fs-extra": "^9.1.0",
    "prettier": "2.3.2",
    "puppeteer": "7.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semantic-release": "17.4.4",
    "semantic-release-slack-bot": "2.1.1",
    "tap": "14.11.0"
  }
}
